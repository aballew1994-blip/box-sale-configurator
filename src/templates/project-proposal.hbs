<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <style>
    * { margin: 0; padding: 0; box-sizing: border-box; }
    body {
      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
      font-size: 11px;
      color: #333;
      line-height: 1.5;
    }

    /* ==================== PAGE SETUP ==================== */
    @page {
      size: Letter;
      margin: 0;
    }

    /* Header/footer are now handled by Puppeteer's headerTemplate/footerTemplate */
    /* These styles are kept for reference but elements are removed from body */

    /* ==================== COVER PAGE ==================== */
    .cover-page {
      position: relative;
      display: flex;
      flex-direction: column;
      page-break-after: always;
      min-height: 100%;
    }

    .red-banner {
      background: #dc2626;
      color: white;
      padding: 8px 24px;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }

    .red-banner .estimate-number {
      font-size: 14px;
      font-weight: 700;
      letter-spacing: 1px;
    }

    .red-banner .label {
      font-size: 12px;
    }

    .cover-content {
      flex: 1;
      padding: 32px;
    }

    .customer-name {
      font-size: 28px;
      font-weight: 700;
      color: #1a365d;
      margin-bottom: 8px;
    }

    .customer-address {
      font-size: 12px;
      color: #666;
      line-height: 1.6;
      margin-bottom: 24px;
    }

    .proposal-dates {
      display: flex;
      gap: 40px;
      margin-bottom: 24px;
    }

    .proposal-dates .date-block {
      font-size: 12px;
    }

    .proposal-dates .date-label {
      color: #666;
      font-size: 10px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 4px;
    }

    .proposal-dates .date-value {
      font-weight: 600;
      color: #1a365d;
      font-size: 14px;
    }

    .title-image-container {
      border-radius: 8px;
      overflow: hidden;
      margin-bottom: 24px;
    }

    .title-image {
      width: 100%;
      height: auto;
      display: block;
    }

    .prepared-by {
      background: #f8fafc;
      padding: 16px 20px;
      border-radius: 8px;
    }

    .prepared-by-label {
      font-size: 10px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      color: #666;
      margin-bottom: 6px;
    }

    .prepared-by-name {
      font-size: 16px;
      font-weight: 600;
      color: #1a365d;
    }

    .prepared-by-title {
      font-size: 12px;
      color: #666;
    }

    /* ==================== CONTENT PAGES ==================== */
    .content-page {
      position: relative;
      padding: 0 32px 24px 32px;
      page-break-after: always;
    }

    .content-page:last-child {
      page-break-after: auto;
    }

    .page-banner {
      background: #dc2626;
      color: white;
      padding: 8px 24px;
      margin: 0 -32px 20px -32px; /* Extend to page edges */
      display: flex;
      justify-content: space-between;
      align-items: center;
    }

    .page-body {
      /* Content flows naturally after banner */
    }

    .page-banner .estimate-number {
      font-size: 14px;
      font-weight: 700;
      letter-spacing: 1px;
    }

    .page-banner .label {
      font-size: 12px;
    }

    /* ==================== SECTION STYLES ==================== */
    .section-title {
      font-size: 16px;
      font-weight: 700;
      color: #1a365d;
      border-bottom: 3px solid #1a365d;
      padding-bottom: 8px;
      margin-bottom: 16px;
    }

    .section-subtitle {
      font-size: 13px;
      font-weight: 600;
      color: #1a365d;
      margin-bottom: 8px;
      margin-top: 20px;
    }

    .section-content {
      font-size: 11px;
      line-height: 1.7;
      color: #444;
      white-space: pre-wrap;
      margin-bottom: 16px;
    }

    .section-intro {
      font-size: 11px;
      color: #666;
      margin-bottom: 12px;
      font-style: italic;
    }

    /* ==================== INFO GRID ==================== */
    .info-grid {
      display: grid;
      grid-template-columns: 1fr 1fr 1fr;
      gap: 24px;
      margin-bottom: 24px;
    }

    .info-block {
      font-size: 11px;
    }

    .info-block .label {
      font-weight: 600;
      color: #1a365d;
      font-size: 10px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 4px;
    }

    .info-block .value {
      font-size: 11px;
      line-height: 1.5;
      color: #444;
    }

    /* ==================== TABLES ==================== */
    table {
      width: 100%;
      border-collapse: collapse;
      font-size: 10px;
    }

    .location-header {
      background: #1a365d;
      color: white;
      padding: 8px 12px;
      font-size: 11px;
      font-weight: 600;
      display: flex;
      justify-content: space-between;
    }

    .location-header.subcontractor {
      background: #4a5568;
    }

    .location-header.unassigned {
      background: #9ca3af;
    }

    thead {
      background: #f3f4f6;
    }

    thead th {
      padding: 6px 8px;
      text-align: left;
      font-weight: 600;
      font-size: 9px;
      text-transform: uppercase;
      letter-spacing: 0.3px;
      color: #6b7280;
    }

    thead th.right { text-align: right; }
    thead th.center { text-align: center; }

    tbody tr {
      border-bottom: 1px solid #e5e7eb;
    }

    tbody tr:nth-child(even) {
      background: #f9fafb;
    }

    tbody td {
      padding: 6px 8px;
      vertical-align: top;
    }

    tbody td.right { text-align: right; }
    tbody td.center { text-align: center; }
    tbody td.desc { max-width: 200px; }
    tbody td.bold { font-weight: 600; }

    /* Materials Total Row */
    .materials-total {
      background: #e5e7eb;
      padding: 8px 12px;
      display: flex;
      justify-content: space-between;
      font-weight: 600;
      font-size: 12px;
      color: #1a365d;
      margin-top: 16px;
    }

    /* ==================== LABOR SECTION ==================== */
    .labor-section {
      margin-top: 24px;
    }

    .labor-header {
      background: #e5e7eb;
      padding: 8px 12px;
      font-weight: 600;
      font-size: 12px;
      color: #1a365d;
      border-radius: 4px 4px 0 0;
    }

    .labor-content {
      border: 1px solid #e5e7eb;
      border-top: none;
      border-radius: 0 0 4px 4px;
      padding: 12px;
    }

    .labor-row {
      display: flex;
      justify-content: space-between;
      padding: 4px 0;
      font-size: 11px;
    }

    .labor-row .label {
      color: #6b7280;
    }

    .labor-row .value {
      font-weight: 500;
    }

    .labor-row.total {
      border-top: 1px solid #e5e7eb;
      margin-top: 8px;
      padding-top: 8px;
    }

    .labor-row.total .label {
      font-weight: 600;
      color: #374151;
    }

    .labor-row.total .value {
      font-weight: 600;
    }

    /* ==================== SUMMARY SECTION ==================== */
    .summary-section {
      margin-top: 24px;
      background: #f8fafc;
      border-radius: 8px;
      padding: 20px;
    }

    .summary-title {
      font-weight: 600;
      font-size: 13px;
      color: #1a365d;
      margin-bottom: 12px;
    }

    .summary-row {
      display: flex;
      justify-content: space-between;
      padding: 4px 0;
      font-size: 11px;
    }

    .summary-row .label {
      color: #6b7280;
    }

    .summary-row .value {
      font-weight: 500;
    }

    .summary-row.subtotal {
      border-top: 1px solid #e5e7eb;
      margin-top: 8px;
      padding-top: 8px;
    }

    .summary-row.total {
      border-top: 2px solid #1a365d;
      margin-top: 8px;
      padding-top: 12px;
    }

    .summary-row.total .label {
      font-weight: 700;
      color: #1a365d;
    }

    .summary-row.total .value {
      font-weight: 700;
      font-size: 14px;
      color: #1a365d;
    }

    .tax-note {
      font-size: 9px;
      color: #9ca3af;
      margin-top: 12px;
      font-style: italic;
    }

    /* ==================== SIGNATURE BLOCK ==================== */
    .signature-section {
      margin-top: 32px;
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 24px;
    }

    .signature-block {
      border: 1px solid #e5e7eb;
      padding: 16px;
      border-radius: 8px;
    }

    .signature-block h4 {
      font-size: 10px;
      font-weight: 600;
      color: #1a365d;
      margin-bottom: 12px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }

    .signature-line {
      display: flex;
      align-items: flex-end;
      margin-bottom: 12px;
      font-size: 9px;
    }

    .signature-line .label {
      color: #6b7280;
      width: 90px;
      flex-shrink: 0;
    }

    .signature-line .line {
      flex: 1;
      border-bottom: 1px solid #333;
      margin-left: 8px;
      min-height: 16px;
    }

    .signature-line .prefilled {
      flex: 1;
      margin-left: 8px;
      color: #333;
    }

    /* ==================== TERMS SECTION ==================== */
    .terms-content {
      column-count: 2;
      column-gap: 32px;
      font-size: 9px;
      line-height: 1.6;
    }

    .terms-content h4 {
      font-size: 10px;
      font-weight: 600;
      color: #1a365d;
      margin-top: 12px;
      margin-bottom: 4px;
    }

    .terms-content h4:first-child {
      margin-top: 0;
    }

    .terms-content p {
      margin-bottom: 8px;
      color: #4b5563;
    }

    /* ==================== PAGE BREAK CONTROL ==================== */
    /* Allow sections to break across pages to prevent cut-off */
    .summary-section {
      page-break-inside: auto;
    }

    .signature-section {
      page-break-inside: avoid;
      page-break-before: auto;
    }

    .labor-section {
      page-break-inside: auto;
    }

    .location-header {
      page-break-after: avoid;
    }

    table {
      page-break-inside: auto;
    }

    tr {
      page-break-inside: avoid;
      page-break-after: auto;
    }

    /* Ensure summary rows don't break in the middle */
    .summary-row {
      page-break-inside: avoid;
    }

    .labor-row {
      page-break-inside: avoid;
    }

    /* ==================== UTILITIES ==================== */
    .mt-4 { margin-top: 16px; }
    .mb-4 { margin-bottom: 16px; }
    .mb-6 { margin-bottom: 24px; }

    .no-items {
      color: #9ca3af;
      font-style: italic;
      padding: 12px;
      text-align: center;
    }
  </style>
</head>
<body>
  <!-- Header/footer are now rendered by Puppeteer's displayHeaderFooter option -->

  <!-- ==================== COVER PAGE ==================== -->
  <div class="cover-page">
    <!-- Red Banner -->
    <div class="red-banner">
      <span class="estimate-number">{{#if estimate}}{{estimate.tranId}}{{else}}{{config.estimateNumber}}{{/if}}</span>
      <span class="label">Project Proposal</span>
    </div>

    <!-- Cover Content -->
    <div class="cover-content">
      <div class="customer-name">
        {{#if customer}}{{customer.name}}{{else}}{{config.customerName}}{{/if}}
      </div>

      {{#if customer}}
      <div class="customer-address">
        {{#if customer.billingAddress}}
        {{customer.billingAddress.addr1}}<br>
        {{#if customer.billingAddress.addr2}}{{customer.billingAddress.addr2}}<br>{{/if}}
        {{customer.billingAddress.city}}, {{customer.billingAddress.state}} {{customer.billingAddress.zip}}
        {{/if}}
      </div>
      {{/if}}

      <div class="proposal-dates">
        <div class="date-block">
          <div class="date-label">Proposal Issued</div>
          <div class="date-value">{{formatDate generatedDate}}</div>
        </div>
        <div class="date-block">
          <div class="date-label">Valid Until</div>
          <div class="date-value">{{formatDate validUntilDate}}</div>
        </div>
      </div>

      <!-- Title Page Image -->
      {{#if images.titlePage}}
      <div class="title-image-container">
        <img src="{{images.titlePage}}" alt="Project Site" class="title-image">
      </div>
      {{/if}}

      <div class="prepared-by">
        <div class="prepared-by-label">Prepared By</div>
        {{#if estimate}}
        <div class="prepared-by-name">{{estimate.salesRep}}</div>
        <div class="prepared-by-title">Account Manager</div>
        {{else}}
        <div class="prepared-by-name">TSI Account Team</div>
        <div class="prepared-by-title">Account Manager</div>
        {{/if}}
      </div>
    </div>
  </div>

  <!-- ==================== STATEMENT OF WORK PAGE ==================== -->
  <div class="content-page">
    <div class="page-banner">
      <span class="estimate-number">{{#if estimate}}{{estimate.tranId}}{{else}}{{config.estimateNumber}}{{/if}}</span>
      <span class="label">Statement of Work</span>
    </div>

    <div class="page-body">
      <div class="section-title">Statement of Work</div>

      <!-- Introduction -->
      {{#if (hasContent config.introduction)}}
      <div class="section-subtitle">Introduction</div>
      <div class="section-content">{{{breaklines config.introduction}}}</div>
      {{/if}}

      <!-- Installation Scope -->
      {{#if (hasContent config.installationScope)}}
      <div class="section-subtitle">Installation Scope</div>
      <div class="section-content">{{{breaklines config.installationScope}}}</div>
      {{/if}}

      <!-- Customer Provided Equipment & Services -->
      {{#if (hasContent config.customerProvidedScope)}}
      <div class="section-subtitle">Customer Provided Equipment & Services</div>
      <div class="section-content">{{{breaklines config.customerProvidedScope}}}</div>
      {{/if}}

      <!-- Professional Services Scope (conditional) -->
      {{#if showProServicesScope}}
      <div class="section-subtitle">Professional Services Scope</div>
      <div class="section-content">{{{breaklines config.proServicesScope}}}</div>
      {{/if}}

      <!-- Solutions Architect Scope (conditional) -->
      {{#if showSolutionsArchitectScope}}
      <div class="section-subtitle">Solutions Architect Scope</div>
      <div class="section-content">{{{breaklines config.solutionsArchitectScope}}}</div>
      {{/if}}

      <!-- Constraints & Assumptions -->
      {{#if (hasContent config.constraintsAssumptions)}}
      <div class="section-subtitle">Constraints & Assumptions</div>
      <div class="section-content">{{{breaklines config.constraintsAssumptions}}}</div>
      {{/if}}

      <!-- Exclusions -->
      {{#if (hasContent config.exclusions)}}
      <div class="section-subtitle">Exclusions</div>
      <div class="section-content">{{{breaklines config.exclusions}}}</div>
      {{/if}}

      <!-- FOCUS Scope (conditional) -->
      {{#if showFocusScope}}
      <div class="section-subtitle">FOCUS Post-Installation Support</div>
      <div class="section-content">{{{breaklines config.focusScope}}}</div>
      {{/if}}
    </div>
  </div>

  <!-- ==================== PROPOSAL DETAILS PAGE ==================== -->
  <div class="content-page">
    <div class="page-banner">
      <span class="estimate-number">{{#if estimate}}{{estimate.tranId}}{{else}}{{config.estimateNumber}}{{/if}}</span>
      <span class="label">Proposal Details</span>
    </div>

    <div class="page-body">
      <div class="section-title">Proposal Details</div>

      <!-- Contact Info Grid -->
      <div class="info-grid">
        <div class="info-block">
          <div class="label">Site Address</div>
          <div class="value">
            {{#if (hasContent config.siteAddressOverride)}}
            {{{breaklines config.siteAddressOverride}}}
            {{else if customer}}
            {{#if customer.shippingAddress}}
            {{customer.shippingAddress.addr1}}<br>
            {{#if customer.shippingAddress.addr2}}{{customer.shippingAddress.addr2}}<br>{{/if}}
            {{customer.shippingAddress.city}}, {{customer.shippingAddress.state}} {{customer.shippingAddress.zip}}
            {{else}}
            See billing address
            {{/if}}
            {{else}}
            Not specified
            {{/if}}
          </div>
        </div>
        <div class="info-block">
          <div class="label">Billing Address</div>
          <div class="value">
            {{#if (hasContent config.billingAddressOverride)}}
            {{{breaklines config.billingAddressOverride}}}
            {{else if customer}}
            {{#if customer.billingAddress}}
            {{customer.billingAddress.addr1}}<br>
            {{#if customer.billingAddress.addr2}}{{customer.billingAddress.addr2}}<br>{{/if}}
            {{customer.billingAddress.city}}, {{customer.billingAddress.state}} {{customer.billingAddress.zip}}
            {{/if}}
            {{else}}
            Not specified
            {{/if}}
          </div>
        </div>
        <div class="info-block">
          <div class="label">Contact</div>
          <div class="value">
            {{#if config.contactFirstName}}
            {{config.contactFirstName}} {{config.contactLastName}}<br>
            {{/if}}
            {{#if config.contactEmail}}
            {{config.contactEmail}}
            {{/if}}
            {{#unless config.contactFirstName}}
            {{#unless config.contactEmail}}
            Not specified
            {{/unless}}
            {{/unless}}
          </div>
        </div>
      </div>

      <!-- Materials by Location -->
      {{#if materialsByLocation.length}}
      {{#each materialsByLocation}}
      <div class="location-header {{#if (eq this.locationName "Subcontractor Materials")}}subcontractor{{/if}}{{#if (eq this.locationName "Miscellaneous")}}unassigned{{/if}}">
        <span>{{this.locationName}}</span>
        <span>{{formatCurrency this.subtotal}}</span>
      </div>
      <table>
        <thead>
          <tr>
            <th style="width: 40px;" class="center">QTY</th>
            <th style="width: 100px;">ITEM</th>
            <th>DESCRIPTION</th>
            <th class="right" style="width: 80px;">UNIT PRICE</th>
            <th class="right" style="width: 70px;">TARIFF</th>
            <th class="right" style="width: 90px;">EXT. PRICE</th>
          </tr>
        </thead>
        <tbody>
          {{#each this.items}}
          <tr>
            <td class="center">{{this.quantity}}</td>
            <td>{{this.partNumber}}</td>
            <td class="desc">{{this.description}}</td>
            <td class="right">{{formatCurrency this.unitPrice}}</td>
            <td class="right">{{formatCurrency this.tariffAmount}}</td>
            <td class="right bold">{{formatCurrency this.extPrice}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      {{/each}}

      <div class="materials-total">
        <span>Materials Total</span>
        <span>{{formatCurrency summary.materialsTotal}}</span>
      </div>
      {{else}}
      <div class="labor-header">Materials</div>
      <div class="labor-content">
        <p class="no-items">No materials have been added to this project.</p>
      </div>
      {{/if}}

      <!-- Labor Section -->
      <div class="labor-section">
        <div class="labor-header">Labor Summary</div>
        <div class="labor-content">
          {{#if laborItems.length}}
          {{#if config.consolidateLabor}}
          <!-- Consolidated Labor View -->
          <div class="labor-row">
            <span class="label">Labor (Consolidated)</span>
            <span class="value">{{formatCurrency summary.laborTotal}}</span>
          </div>
          {{else}}
          <!-- Itemized Labor View -->
          {{#each laborItems}}
          <div class="labor-row">
            <span class="label">{{this.description}}</span>
            <span class="value">{{formatCurrency this.extPrice}}</span>
          </div>
          {{/each}}
          <div class="labor-row total">
            <span class="label">Labor Total</span>
            <span class="value">{{formatCurrency summary.laborTotal}}</span>
          </div>
          {{/if}}
          {{else}}
          <p class="no-items">No labor has been added to this project.</p>
          {{/if}}
        </div>
      </div>

      <!-- Financial Summary -->
      <div class="summary-section">
        <div class="summary-title">Financial Summary</div>

        <div class="summary-row">
          <span class="label">Materials Total</span>
          <span class="value">{{formatCurrency summary.materialsTotal}}</span>
        </div>
        {{#if summary.tariffTotal}}
        <div class="summary-row">
          <span class="label">Tariff Total</span>
          <span class="value">{{formatCurrency summary.tariffTotal}}</span>
        </div>
        {{/if}}
        <div class="summary-row">
          <span class="label">Labor Total</span>
          <span class="value">{{formatCurrency summary.laborTotal}}</span>
        </div>
        <div class="summary-row subtotal">
          <span class="label">Subtotal</span>
          <span class="value">{{formatCurrency summary.subtotal}}</span>
        </div>
        {{#if summary.indirectCost}}
        <div class="summary-row">
          <span class="label">Indirect Costs</span>
          <span class="value">{{formatCurrency summary.indirectCost}}</span>
        </div>
        {{/if}}
        <div class="summary-row total">
          <span class="label">Purchase Price (Excl. Taxes)</span>
          <span class="value">{{formatCurrency summary.grandTotal}}</span>
        </div>

        <p class="tax-note">
          * Sales tax will be calculated and added to the final invoice based on the project location and applicable tax rates.
        </p>
      </div>

      <!-- Signature Block -->
      <div class="signature-section">
        <div class="signature-block">
          <h4>Client Acceptance</h4>
          <div class="signature-line">
            <span class="label">Date Accepted:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Signature:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Printed Name:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Title:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">PO Number:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Email:</span>
            <span class="line"></span>
          </div>
        </div>

        <div class="signature-block">
          <h4>TSI Representative</h4>
          <div class="signature-line">
            <span class="label">Date Submitted:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Signature:</span>
            <span class="line"></span>
          </div>
          <div class="signature-line">
            <span class="label">Submitted By:</span>
            {{#if estimate}}
            <span class="prefilled">{{estimate.salesRep}}</span>
            {{else}}
            <span class="line"></span>
            {{/if}}
          </div>
          <div class="signature-line">
            <span class="label">Title:</span>
            <span class="prefilled">Account Manager</span>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- ==================== TERMS OF SALE PAGE ==================== -->
  <div class="content-page">
    <div class="page-banner">
      <span class="estimate-number">{{#if estimate}}{{estimate.tranId}}{{else}}{{config.estimateNumber}}{{/if}}</span>
      <span class="label">Terms of Sale</span>
    </div>

    <div class="page-body">
      <div class="section-title">Terms of Sale</div>

      <div class="terms-content">
        <h4>Order Processing</h4>
        <p>
